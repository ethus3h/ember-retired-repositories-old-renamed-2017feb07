            <type name="guint64" c:type="guint64"/>
          </parameter>
          <parameter name="size" transfer-ownership="none">
            <doc xml:whitespace="preserve">The length of the buffer</doc>
            <type name="guint" c:type="guint"/>
          </parameter>
          <parameter name="buffer"
                     direction="out"
