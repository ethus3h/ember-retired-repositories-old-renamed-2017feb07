        <return-value transfer-ownership="none">
          <doc xml:whitespace="preserve">the #GstStateChangeReturn of the state transition.</doc>
          <type name="StateChangeReturn" c:type="GstStateChangeReturn"/>
        </return-value>
        <parameters>
