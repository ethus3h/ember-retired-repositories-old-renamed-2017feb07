          <type name="Message" c:type="GstMessage*"/>
        </return-value>
        <parameters>
          <instance-parameter name="bus" transfer-ownership="none">
            <doc xml:whitespace="preserve">a #GstBus</doc>
            <type name="Bus" c:type="GstBus*"/>
